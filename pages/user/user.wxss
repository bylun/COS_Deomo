.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  color: #3f3939;
}

.userinfo {
  height: 200rpx;
  width: 100%;
  background: linear-gradient(#1488ba, #1cbbb4);
}

.user-avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
}

.nickName {
  font-size: 38rpx;
}

.login-btn{
  background-color: #0a95e4;
  color: #fff;
}

.cub-list {
  width: 100%;
}

.cub-item {
  height: 80rpx;
  width: 100%;
  font-size: 36rpx;
  padding: 10rpx 40rpx 0 40rpx;
}

.iconfont {
  font-size: 40rpx;
}

.version{
  font-size: 34rpx;
}
